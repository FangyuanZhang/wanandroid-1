/**在使用的Wxss中引入WxParse.css,可以在app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background-color: #f5f5f5;
}

.page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}


/**字体样式**/
.text_w_w_black16 {
  font-size: 16px;
  color: #000;
  padding: 10px;
}

.text_f_w_black16 {
  width: 100%;
  height: auto;
  font-size: 16px;
  color: #000;
  padding: 10px;
}

.text_w_w_black14 {
  font-size: 14px;
  color: #000;
  padding: 10px;
}